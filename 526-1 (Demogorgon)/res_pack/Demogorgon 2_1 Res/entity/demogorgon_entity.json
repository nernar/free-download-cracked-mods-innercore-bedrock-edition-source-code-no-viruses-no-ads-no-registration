{
  "format_version": "1.8.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "entity:demogorgon",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "npc"
      },
      "textures": {
        "default": "textures/entity/demogorgon/demogorgon",
        "open": "textures/entity/demogorgon/demogorgonopen"
      },
      "geometry": {
        "default": "geometry.demogorgon"
      },
      "scripts": {
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.tcos1 = -variable.tcos0;",
          "variable.attack_body_rot_y = Math.sin(Math.sqrt(variable.attack_time) * 360.0) * 11.46;",
          "variable.cos_attack_body_rot_y = Math.cos(variable.attack_body_rot_y);",
          "variable.sin_attack_body_rot_y = Math.sin(variable.attack_body_rot_y);",
          "variable.internal_swim_pos = Math.mod(query.modified_distance_moved, 26.0);",
          "variable.attack = Math.sin((1.0 - (1.0 - variable.attack_time) * (1.0 - variable.attack_time)) * 180.0) * 57.3;",
          "variable.attack2 = Math.sin(variable.attack_time * 180.0) * 57.3;",
          "variable.z_bob = Math.cos(query.life_time * 103.13244) * 2.865 + 2.865;",
          "variable.x_bob = Math.sin(query.life_time * 76.776372) * 2.865;"
        ]
      },
      "animations": {
          
        "demogorgon_general": "animation.demogorgon.general",
        "demogorgon_big_head": "animation.demogorgon.big_head",
        
        "demogorgon_general_2": "animation.demogorgon.general_2",
        "demogorgon_general_legs": "animation.demogorgon.general_legs",
        
        "look_at_target": "animation.demogorgon.look_at_target",
        "move": "animation.demogorgon.move",
        "look_at_target_head": "animation.demogorgon.look_at_target_head",
        "casting": "animation.demogorgon.casting",
        "attack_bare_hand": "animation.demogorgon.attack_bare_hand"
},
      "animation_controllers": [
        { "demogorgon_baby_big_head": "controller.animation.demogorgon.baby_big_head" },
        { "general": "controller.animation.demogorgon.general" },
        { "move": "controller.animation.demogorgon.move" },
        { "zombie_attack_bare_hand": "controller.animation.demogorgon.attack_bare_hand" }
      ],
      "render_controllers": [ "controller.render.demogorgon" ],
      "spawn_egg": {
        "base_color": "#ff694343",
        "overlay_color": "#ff8e6a6a"
      }
    }
  }
}