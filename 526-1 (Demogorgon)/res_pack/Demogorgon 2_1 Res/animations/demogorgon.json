    "animation.demogorgon.attack_bare_hand": {
      "loop": true,
      "bones": {
        "rightArm1": {
          "rotation": [
            "Math.sin(1.0 - Math.pow(1.0 - variable.attack_time, -10.0) * 180.0) * (variable.is_brandishing_spear ? -1.0 : 1.0 )",
            "variable.is_brandishing_spear ? 0.0 : variable.attack_body_rot_y * 2.0",
            0.0
          ]
        },
        "leftarm1": {
          "rotation": [
            "Math.sin(1.0 - Math.pow(1.0 - variable.attack_time, 10.0) * 180.0) * (variable.is_brandishing_spear ? -1.0 : 1.0 )",
