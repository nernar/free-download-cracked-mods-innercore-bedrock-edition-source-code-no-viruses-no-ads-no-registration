{
  "format_version": "1.8.0",
  "minecraft:entity": {
    "description": {
      "identifier": "test:man",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "test:stop": {
        "minecraft:movement": {
          "value": 0.0
         }
      },
      "test:continue": {
        "minecraft:movement": {
          "value": 0.25
         }
      }
   },
    "components": {
      "minecraft:type_family": {
        "family": [ "player" ]
      },
      "minecraft:behavior.look_at_player": {
        "priority": 1,
        "look_distance": 5.0,
        "probability": 1.0
      },
      "minecraft:collision_box": {
        "width": 0.5,
        "height": 1.975
      },
      "minecraft:health": {
        "value": 30,
        "max": 30
      },
       "minecraft:behavior.melee_attack": {
         "melee_fov": 135,
         "speed_multiplier": 1.25,
         "track_target": true
      },
       "minecraft:target_nearby_sensor": {
         "inside_range": 1.0,
         "outside_range": 5.0,
         "on_inside_range": {
           "event": "test:stop"
        },
        "on_outside_range": {
          "event": "test:continue"
        }
      },
      "minecraft:attack": {
         "damage": 7
      },
      "minecraft:nameable": {
      },
      "minecraft:physics": {
      },
      "minecraft:behavior.random_stroll": {
        "speed_multiplier": 1
      },
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:jump.static": {
      },
      "minecraft:behavior.random_look_around": {
        "priority": 7
      },
      "minecraft:movement.basic": {
      },
      "minecraft:navigation.walk": {
        "can_pass_doors": true,
        "can_walk": true,
        "can_break_doors": false,
        "can_float": true,
        "avoid_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 4,
        "within_radius": 15,
        "reselect_targets": true,
        "entity_types": [
          {
            "filters": {
              "any_of": [
                { "test": "is_family", "subject": "other", "value": "zombie" },
                { "test": "is_family", "subject": "other", "value": "skeleton" }
              ]
            },
            "max_dist": 15
          }
        ],
        "must_see": true,
        "must_see_forget_duration": 17.0
      }
    },
    "events": {
      "test:stop": {
        "remove": {
          "component_groups": [ "test:continue" ]
        },
        "add": {
          "component_groups": [ "test:stop" ]
        }
      },
      "test:continue": {
        "remove": {
          "component_groups": [ "test:stop" ]
        },
        "add": {
          "component_groups": [ "test:continue" ]
           }
        }
     }
  }
}