{
  "format_version": "1.16.100",
  "minecraft:entity": {
    "description": {
      "identifier": "bioplentia:duck",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {

      "bioplentia:waiting": {

         "minecraft:timer": {
           "looping": true,
           "time": 100,
           "time_down_event": {
             "event": "bioplentia:waiting_timeout"
            }
          }
       },

      "bioplentia:look_for_target": {
         "minecraft:behavior.move_to_block": {
           "priority": 10,
           "tick_interval": 1,
           "start_chance": 0.2,
           "search_range": 12,
           "search_height": 32,
           "goal_radius": 1.0,
           "stay_duration": 20.0,
           "target_selection_method": "random",
           "target_offset": [0, 0.3, 0],
           "target_blocks": [
             "minecraft:waterlily",
             "bioplentia:cattail"
           ],
           "on_stay_completed": [
             {
               "event": "bioplentia:tired",
               "target": "self"
               }
             ]
           },
           "minecraft:timer": {
             "looping": true,
             "time": 180,
             "time_down_event": {
               "event": "bioplentia:find_target_timeout"
              }
          }
      },

      "bioplentia:duck_baby": {
        "minecraft:is_baby": {
        },
        "minecraft:scale": {
          "value":0.5
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feed_items": [
            "wheat_seeds",
            "beetroot_seeds",
            "melon_seeds",
            "pumpkin_seeds"
          ],
          "grow_up": {
            "event": "minecraft:ageable_grow_up",
            "target": "self"
          }
        },
        "minecraft:behavior.follow_parent": {
          "priority": 5,
          "speed_multiplier": 1.1
        }
      },

      "bioplentia:duck_adult": {
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(1,3)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/chicken.json"
        },
        "minecraft:breedable": {
          "require_tame": false,
          "breeds_with": {
            "mate_type": "bioplentia:duck",
            "baby_type": "bioplentia:duck",
            "breed_event": {
              "event": "minecraft:entity_born",
              "target": "baby"
            }
          },
          "breed_items": [
            "wheat_seeds",
            "beetroot_seeds",
            "melon_seeds",
            "pumpkin_seeds"
          ]
        },
        "minecraft:behavior.breed": {
          "priority": 3,
          "speed_multiplier": 1.0
        },
        "minecraft:rideable": {
          "seat_count": 1,
          "family_types": [
            "zombie"
          ],
          "seats": {
            "position": [ 0.0, 0.4, 0.0 ]
          }
        }
      }
    },

    "components": {
      "minecraft:type_family": {
        "family": [ "duck", "mob" ]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0,
        "generates_bubbles": false
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 0.8
      },
      "minecraft:nameable": {
      },
      "minecraft:health": {
        "value": 10,
        "max": 10
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "cause": "fall",
          "deals_damage": false
        }
      },
      "minecraft:leashable": {
        "soft_distance": 4.0,
        "hard_distance": 6.0,
        "max_distance": 10.0
      },
      "minecraft:balloonable": {
        "mass": 0.5
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.panic": {
        "priority": 1,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.mount_pathing": {
        "priority": 2,
        "speed_multiplier": 1.5,
        "target_dist": 0.0,
        "track_target": true
      },
      "minecraft:behavior.tempt": {
        "priority": 4,
        "speed_multiplier": 1.0,
        "items": [
          "wheat_seeds",
          "beetroot_seeds",
          "melon_seeds",
          "pumpkin_seeds"
        ]
      },
      "minecraft:behavior.random_stroll": {
        "priority": 6,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.look_at_player": {
        "priority": 7,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:behavior.random_look_around": {
        "priority": 8
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {
      }
    },

    "events": {
     "bioplentia:find_target_timeout": {
       "remove": {
         "component_groups": [
           "bioplentia:look_for_target"
          ]
        },
        "add": {
          "component_groups": [
            "bioplentia:adult"
         ]
       }
     },
     "bioplentia:tired": {
         "remove": {
            "component_groups": [
               "bioplentia:look_for_target"
             ]
          },
          "add": {
            "component_groups": [

            "bioplentia:waiting"
          ]
        }
      },
      "bioplentia:waiting_timeout": {
         "remove": {
            "component_groups": [
               "bioplentia:waiting"
             ]
          },
          "add": {
            "component_groups": [

            "bioplentia:look_for_target"
          ]
        }
      },
      "minecraft:entity_spawned": {
        "randomize": [
          {
            "weight": 95,
            "remove": {
            },
            "add": {
              "component_groups": [
                "bioplentia:duck_adult"
              ]
            }
          },
          {
            "weight": 5,
            "remove": {
            },
            "add": {
              "component_groups": [
                "bioplentia:duck_baby"
              ]
            }
          }
        ]
      },

      "minecraft:entity_born": {
        "remove": {
        },
        "add": {
          "component_groups": [
            "bioplentia:duck_baby"
          ]
        }
      },

      "minecraft:ageable_grow_up": {
        "remove": {
          "component_groups": [
            "bioplentia:duck_baby"
          ]
        },
        "add": {
          "component_groups": [
            "bioplentia:duck_adult"
          ]
        }
      }
    }
  }
}