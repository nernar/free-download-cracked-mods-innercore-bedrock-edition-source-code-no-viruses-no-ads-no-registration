{
    "format_version": "1.8.0",
    "minecraft:entity": {
        "description": {
            "identifier": "lotr:manatees",
            "runtime_identifier": "minecraft:dolphin",
            "is_spawnable": true,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "minecraft:manatees_adult": {
                "minecraft:loot": {
                    "table": "loot_tables/entities/loot_manatees.json"
                },
                "minecraft:scale": {
                    "value": 0.8
                },
                "minecraft:breedable": {
                    "requireTame": false,
                    "breedItems": "kelp",
                    "breedsWith": {
                        "mateType": "lotr:manatees",
                        "babyType": "lotr:manatees",
                        "breed_event": {
                            "event": "minecraft:entity_born",
                            "target": "baby"
                        }
                    }
                }
            },
            "minecraft:manatees_baby": {
                "minecraft:is_baby": {
                },
                "minecraft:scale": {
                    "value": 0.5
                },
                "minecraft:ageable": {
                    "duration": 1200,
                    "feedItems": [ "seagrass" ],
                    "grow_up": {
                        "event": "minecraft:ageable_grow_up",
                        "target": "self"
                    }
                },
                "minecraft:behavior.follow_parent": {
                    "priority": 4,
                    "speed_multiplier": 1.1
                }
            },
            "minecraft:manatees_dried": {
                "minecraft:damage_over_time": {
                    "damagePerHurt": 1,
                    "timeBetweenHurt": 0
                }
            },
            "minecraft:manatees_swimming_navigation": {
                "minecraft:behavior.look_at_player": {
                    "priority": 8,
                    "look_distance": 6
                },
                "minecraft:navigation.generic": {
                    "is_amphibious": true,
                    "can_path_over_water": false,
                    "can_swim": true,
                    "can_walk": false,
                    "can_breach": true,
                    "can_sink": false
                },
                "minecraft:environment_sensor": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "on_ground",
                                    "operator": "==",
                                    "value": true
                                },
                                {
                                    "test": "in_water",
                                    "operator": "!=",
                                    "value": true
                                }
                            ]
                        },
                        "event": "minecraft:navigation_on_land"
                    }
                ]
            },
            "minecraft:manatees_on_land": {
                "minecraft:navigation.generic": {
                    "is_amphibious": true,
                    "can_path_over_water": true,
                    "can_swim": true,
                    "can_walk": true,
                    "can_breach": false,
                    "can_jump": false
                },
                "minecraft:timer": {
                    "looping": false,
                    "time": 40,
                    "time_down_event": {
                        "event": "minecraft:dried_out"
                    }
                },
                "minecraft:environment_sensor": [
                    {
                        "filters": {
                            "test": "in_water_or_rain",
                            "operator": "==",
                            "value": true
                        },
                        "event": "minecraft:stop_dryingout"
                    }
                ]
            },
            "minecraft:manatees_on_land_in_rain": {
                "minecraft:navigation.generic": {
                    "is_amphibious": true,
                    "can_path_over_water": true,
                    "can_swim": true,
                    "can_walk": true,
                    "can_breach": false,
                    "can_jump": false
                },
                "minecraft:environment_sensor": [
                    {
                        "filters": {
                            "test": "in_water",
                            "operator": "==",
                            "value": true
                        },
                        "event": "minecraft:navigation_off_land"
                    },
                    {
                        "filters": {
                            "test": "in_water_or_rain",
                            "operator": "!=",
                            "value": true
                        },
                        "event": "minecraft:start_dryingout"
                    }
                ]
            },
            "minecraft:manatees_leashed": {
                "minecraft:behavior.move_towards_restriction": {
                    "priority": 2,
                    "speed_multiplier": 1.0
                }
            }
        },

        "components": {
            "minecraft:nameable": {
            },
            "minecraft:type_family": {
                "family": [ "manatees" ]
            },
            "minecraft:behavior.tempt": {
                "priority": 4,
                "speed_multiplier": 1.25,
                "items": [
                    "seagrass",
                    "kelp"
                ]
            },
            "minecraft:behavior.panic": {
                "priority": 1,
                "speed_multiplier": 1.25
            },
            "minecraft:leashable": {
                "soft_distance": 4.0,
                "hard_distance": 6.0,
                "max_distance": 10.0,
                "on_leash": {
                    "event": "minecraft:on_leash",
                    "target": "self"
                },
                "on_unleash": {
                    "event": "minecraft:on_unleash",
                    "target": "self"
                }
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "filters": {
                            "test": "in_lava",
                            "subject": "self",
                            "operator": "==",
                            "value": true
                        },
                        "cause": "lava",
                        "damage_per_tick": 4
                    }
                ]
            },
            "minecraft:collision_box": {
                "width": 0.8,
                "height": 1.4
            },
            "minecraft:health": {
                "value": 30,
                "max": 30
            },
            "minecraft:follow_range": {
                "value": 48,
                "max": 48
            },
            "minecraft:breathable": {
                "totalSupply": 240,
                "suffocateTime": 0,
                "breathesAir": true,
                "breathesWater": true,
                "generatesBubbles": false
            },
            "minecraft:movement": {
                "value": 0.1
            },
            "minecraft:navigation.generic": {
                "is_amphibious": true,
                "can_path_over_water": true,
                "can_swim": true,
                "can_walk": false,
                "can_breach": true,
                "can_sink": false
            },
            "minecraft:underwater_movement": {
                "value": 0.15
            },
            "minecraft:jump.static": {
                "jump_power": 0.2
            },
            "minecraft:can_climb": {
            },
            "minecraft:behavior.random_swim": {
                "priority": 5,
                "interval": 0,
                "xz_dist": 20
            },
            "minecraft:behavior.random_breach": {
                "priority": 4,
                "interval": 50,
                "xz_dist": 6,
                "cooldown_time": 2.0
            },
            "minecraft:behavior.swim_wander": {
                "priority": 4,
                "speed_multiplier": 1.0,
                "interval": 10,
                "look_ahead": 2.0
            },
            "behavior.random_look_around": {
                "priority": 7
            },
            "minecraft:behavior.avoid_mob_type": {
                "priority": 2,
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "guardian_elder"
                                },
                                {
                                    "test": "is_family",
                                    "subject": "other",
                                    "value": "guardian"
                                }
                            ]
                        },
                        "max_dist": 8,
                        "walk_speed_multiplier": 1.0,
                        "sprint_speed_multiplier": 1.0
                    }
                ],
                "probability_per_strength": 0.14
            },
            "minecraft:behavior.move_to_water": {
                "priority": 1,
                "search_range": 15,
                "search_height": 5
            },
            "minecraft:behavior.breathe_air": {
                "priority": 4
            },
            "minecraft:on_target_acquired": {
                "event": "minecraft:become_angry",
                "target": "self"
            },
            "minecraft:on_target_escape": {
                "event": "minecraft:on_calm",
                "target": "self"
            },
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:physics": {
            }
        },

        "events": {
            "minecraft:entity_spawned": {
                "randomize": [
                    {
                        "weight": 90,
                        "add": {
                            "component_groups": [
                                "minecraft:manatees_adult",
                                "minecraft:manatees_swimming_navigation"
                            ]
                        }
                    },
                    {
                        "weight": 10,
                        "add": {
                            "component_groups": [
                                "minecraft:manatees_baby",
                                "minecraft:manatees_swimming_navigation"
                            ]
                        }
                    }
                ]
            },
            "minecraft:ageable_grow_up": {
                "remove": {
                    "component_groups": [
                        "minecraft:manatees_baby"
                    ]
                },
                "add": {
                    "component_groups": [
                        "minecraft:manatees_adult"
                    ]
                }
            },
            "minecraft:stop_dryingout": {
                "remove": {
                    "component_groups": [
                        "minecraft:manatees_on_land",
                        "minecraft:manatees_dried"
                    ]
                },
                "add": {
                    "component_groups": [ "minecraft:manatees_on_land_in_rain" ]
                }
            },
            "minecraft:start_dryingout": {
                "remove": {
                    "component_groups": [ "minecraft:manatees_on_land_in_rain" ]
                },
                "add": {
                    "component_groups": [
                        "minecraft:manatees_on_land"
                    ]
                }
            },
            "minecraft:dried_out": {
                "add": {
                    "component_groups": [ "minecraft:manatees_dried" ]
                }
            },
            "minecraft:navigation_on_land": {
                "add": {
                    "component_groups": [ "minecraft:manatees_on_land" ]
                },
                "remove": {
                    "component_groups": [ "minecraft:manatees_swimming_navigation" ]
                }
            },
            "minecraft:navigation_off_land": {
                "add": {
                    "component_groups": [ "minecraft:manatees_swimming_navigation" ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:manatees_on_land_in_rain",
                        "minecraft:manatees_on_land",
                        "minecraft:manatees_dried"
                    ]
                }
            },
            "minecraft:on_leash": {
                "add": {
                    "component_groups": [
                        "minecraft:manatees_leashed"
                    ]
                }
            },
            "minecraft:on_unleash": {
                "remove": {
                    "component_groups": [
                        "minecraft:manatees_leashed"
                    ]
                }
            },
            "minecraft:entity_born": {
                "add": {
                    "component_groups": [
                        "minecraft:manatees_baby"
                    ]
                }
            }
        }
    }
}