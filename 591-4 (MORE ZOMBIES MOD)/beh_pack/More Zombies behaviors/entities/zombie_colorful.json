      },
        "minecraft:timer": {
	    "time": [1, 1],
		"looping": true,
		"time_down_event": {
		"event": "step_3"
		}
	  },
        "minecraft:variant": {
          "value": 3
        }
      },
      "colorful_4": {
        "minecraft:attack": {
        "damage": 4,
        "effect_name": "poison",
        "effect_duration": 20
      },
        "minecraft:timer": {
	    "time": [1, 1],
		"looping": true,
		"time_down_event": {
		"event": "step_4"
		}
	  },
        "minecraft:variant": {
          "value": 4
        }
      },
      "colorful_5": {
        "minecraft:attack": {
        "damage": 4,
        "effect_name": "moveSlowdown",
        "effect_duration": 20
      },
        "minecraft:timer": {
	    "time": [1, 1],
		"looping": true,
		"time_down_event": {
		"event": "step_5"
