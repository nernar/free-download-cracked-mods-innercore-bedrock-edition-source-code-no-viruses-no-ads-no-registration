const info_alert = "i am a lolicon";

if(__config__.get("debug.alert_log")){
UI.getContext().runOnUiThread(({
run: function(){try {
var d = new android.app.AlertDialog.Builder(UI.getContext())
d.setNegativeButton("Yes",new android.content.DialogInterface.OnClickListener(){onClick: function(){}});
d.setTitle(Translation.translate("Ice And Fire"));
d.setMessage(
Translation.translate(info_alert)
);
d.show()
}catch(e){}
}}));
};









// file: Api/ToolsApi.js

ToolAPI.breakCarriedTool = function(damage){
	var item = Player.getCarriedItem(true);
	item.data += damage;
	if(item.data > Item.getMaxDamage(item.id)){
		item.id = 0;
	}
	Player.setCarriedItem(item.id, item.count, item.data, item.enchant);
}





