        var blc15 = lightning_forge15.id == BlockID.dragon_bone_block;
        
        var blc16 = lightning_forge16.id == BlockID.dragon_forg_lightning_brick;
        var blc17 = lightning_forge17.id == BlockID.dragon_forg_lightning_brick;
        var blc18 = lightning_forge18.id == BlockID.dragon_forg_lightning_brick;
        
        var blc19 = lightning_forge19.id == BlockID.dragon_bone_block;
        var blc20 = lightning_forge20.id == BlockID.dragon_forg_lightning_brick;
        var blc21 = lightning_forge21.id == BlockID.dragon_bone_block;
        
        var blc22 = lightning_forge22.id == BlockID.dragon_forg_lightning_brick;
        var blc23 = lightning_forge23.id == BlockID.dragon_forg_lightning_brick;
        var blc24 = lightning_forge24.id == BlockID.dragon_forg_lightning_brick;
        
        var blc25 = lightning_forge25.id == BlockID.dragon_forg_lightning_brick;
        var blc26 = lightning_forge26.id == BlockID.dragon_forg_lightning_brick;
        
        
        
        
        
	    if(blc1 && blc2 && blc3 && blc4 && blc5 && blc6 && blc7 && blc8 && blc9 && blc10 && blc11 && blc12 && blc13 && blc14 && blc15 && blc16 && blc17 && blc18 && blc19 && blc20 && blc21&& blc22 && blc23 && blc24 && blc25){
		
		
		World.setBlock(coords.x+1, coords.y-1, coords.z-1, BlockID.dragon_bone_block_fake, 0, false); 
World.setBlock(coords.x+1, coords.y, coords.z-1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x+1, coords.y+1, coords.z-1, BlockID.dragon_bone_block_fake, 0, false); 

World.setBlock(coords.x, coords.y-1, coords.z-1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x, coords.y, coords.z-1, BlockID.dragon_forg_lightning_furnace_fake, 0, false); 
World.setBlock(coords.x, coords.y+1, coords.z-1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 

World.setBlock(coords.x-1, coords.y-1, coords.z-1, BlockID.dragon_bone_block_fake, 0, false); 
World.setBlock(coords.x-1, coords.y, coords.z-1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x-1, coords.y+1, coords.z-1, BlockID.dragon_bone_block_fake, 0, false); 

World.setBlock(coords.x-1, coords.y-1, coords.z, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x-1, coords.y, coords.z, BlockID.dragon_forg_lightning_furnace_fake, 0, false); 
World.setBlock(coords.x-1, coords.y+1, coords.z, BlockID.dragon_forg_lightning_brick_fake, 0, false); 

World.setBlock(coords.x-1, coords.y-1, coords.z+1, BlockID.dragon_bone_block_fake, 0, false); 
World.setBlock(coords.x-1, coords.y, coords.z+1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x-1, coords.y+1, coords.z+1, BlockID.dragon_bone_block_fake, 0, false); 

World.setBlock(coords.x, coords.y-1, coords.z+1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x, coords.y, coords.z+1, BlockID.dragon_forg_lightning_furnace_fake, 0, false); 
World.setBlock(coords.x, coords.y+1, coords.z+1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 

World.setBlock(coords.x+1, coords.y-1, coords.z+1, BlockID.dragon_bone_block_fake, 0, false); 
World.setBlock(coords.x+1, coords.y, coords.z+1, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x+1, coords.y+1, coords.z+1, BlockID.dragon_bone_block_fake, 0, false); 

World.setBlock(coords.x+1, coords.y-1, coords.z, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x+1, coords.y, coords.z, 0, 0, false); 
World.setBlock(coords.x+1, coords.y+1, coords.z, BlockID.dragon_forg_lightning_brick_fake, 0, false); 

World.setBlock(coords.x, coords.y-1, coords.z, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
World.setBlock(coords.x, coords.y+1, coords.z, BlockID.dragon_forg_lightning_brick_fake, 0, false); 
		World.setBlock(coords.x, coords.y, coords.z, BlockID.dragon_forg_lightning_core, 0, false);
		
		}
	}
};




