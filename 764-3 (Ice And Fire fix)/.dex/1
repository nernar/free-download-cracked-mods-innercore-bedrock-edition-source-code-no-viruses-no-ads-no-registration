var BUTTON_GUI = new UI.Window({
    location: {
        x: __config__.getNumber("debug.bauble_x"),
        y: UI.getScreenHeight() - 70,
        width: 60,
        height: 60
    },
    drawing: [],
    elements: {
        "btn": {
            type: "button",
            x: 0,
            y: 0,
            bitmap: "button.open_btn",
            bitmap2: "button.open_btn_pressed",
            scale: 1000 / 26,
            clicker: {
                onClick: function () {
                    Baubles.openGui();
                }
            }
        }
    }
