/*__FunctionDragon__*/

//TileEntity.registerPrototype(BlockID.dragon_fire_dead, {
    //click: function(id){
Block.registerClickFunction("dragon_fire_dead", function(coords, player){
    World.drop(coords.x, coords.y, coords.z, ItemID.scale_red, 13, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.fire_dragon_meat, 5, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.fire_dragon_heart, 1, 0);
    World.setBlock(coords.x, coords.y, coords.z, BlockID.dragon_fire_dead_skeleton, 0); 
    
});
//TileEntity.registerPrototype(BlockID.dragon_ice_dead, {
	//click: function(id){
Block.registerClickFunction("dragon_ice_dead", function(coords, player){
    World.drop(coords.x, coords.y, coords.z, ItemID.scale_blue, 13, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.ice_dragon_meat, 5, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.ice_dragon_heart, 1, 0);
    World.setBlock(coords.x, coords.y, coords.z, BlockID.dragon_ice_dead_skeleton, 0); 
    
});

//TileEntity.registerPrototype(BlockID.dragon_lightning_dead, {
	//click: function(id){
Block.registerClickFunction("dragon_lightning_dead", function(coords, player){
    World.drop(coords.x, coords.y, coords.z, ItemID.scale_electric, 13, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.lightning_dragon_meat, 5, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.lightning_dragon_heart, 1, 0);
    World.setBlock(coords.x, coords.y, coords.z, BlockID.dragon_lightning_dead_skeleton, 0); 
    
});








/*__FunctionsDragonSkeleton__*/


Block.registerClickFunction("dragon_fire_dead_skeleton", function(coords){
    World.drop(coords.x, coords.y, coords.z, ItemID.dragon_bone, 14, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.dragon_skull_fire, 1, 0);
    World.setBlock(coords.x, coords.y, coords.z, 0, 0); 
});


Block.registerClickFunction("dragon_ice_dead_skeleton", function(coords){
    World.drop(coords.x, coords.y, coords.z, ItemID.dragon_bone, 14, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.dragon_skull_ice, 1, 0);
    World.setBlock(coords.x, coords.y, coords.z, 0, 0); 
});


Block.registerClickFunction("dragon_lightning_dead_skeleton", function(coords){
    World.drop(coords.x, coords.y, coords.z, ItemID.dragon_bone, 14, 0);
    World.drop(coords.x, coords.y, coords.z, ItemID.dragon_skull_lightning, 1, 0);
    World.setBlock(coords.x, coords.y, coords.z, 0, 0); 
});

/*__EntitySpawnModel__*/

Callback.addCallback("EntityDeath", function(entity, player){
var E = Entity.getTypeName(entity);
var coords = Entity.getPosition(entity);
if(E == "test:fire_dragon<>" ){
   World.setBlock(coords.x, coords.y, coords.z, BlockID.dragon_fire_dead, 0); 
   dragonFireDie.play();
   ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "kill_dragon", true);__complete__.play();
   
}
if(E == "test:ice_dragon<>"){
	World.setBlock(coords.x, coords.y, coords.z, BlockID.dragon_ice_dead, 0); 
	dragonIceDie.play();
	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "kill_dragon", true);__complete__.play();
	
}
if(E == "test:electric_dragon<>"){
	World.setBlock(coords.x, coords.y, coords.z, BlockID.dragon_lightning_dead, 0); 
	dragonLightningDie.play();
	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "kill_dragon", true);__complete__.play();
	
}
});

    




// file: Core/Entity/MobDrop.js

Callback.addCallback("EntityDeath", function(entity){
var E = Entity.getType(entity);
var coords = Entity.getPosition(entity);

 if(E == 48 ){
      World.drop(coords.x, coords.y, coords.z, ItemID.wither_bone, 1, 0);
}
});




// file: Core/Function/Break.js

Callback.addCallback("DestroyBlock", function (coords, block) {
	if(block.id == BlockID.dragon_forg_fire_core){
		World.setBlock(coords.x+1, coords.y-1, coords.z-1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x+1, coords.y, coords.z-1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z-1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x, coords.y-1, coords.z-1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x, coords.y, coords.z-1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z-1, BlockID.dragon_forg_fire_brick, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z-1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x-1, coords.y, coords.z-1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z-1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x-1, coords.y, coords.z, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z, BlockID.dragon_forg_fire_brick, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z+1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x-1, coords.y, coords.z+1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z+1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x, coords.y-1, coords.z+1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x, coords.y, coords.z+1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z+1, BlockID.dragon_forg_fire_brick, 0); 

World.setBlock(coords.x+1, coords.y-1, coords.z+1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x+1, coords.y, coords.z+1, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z+1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x+1, coords.y-1, coords.z, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x+1, coords.y, coords.z, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z, BlockID.dragon_forg_fire_brick, 0); 

World.setBlock(coords.x, coords.y-1, coords.z, BlockID.dragon_forg_fire_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z, BlockID.dragon_forg_fire_brick, 0); 
		}
if(block.id == BlockID.dragon_forg_ice_core){
		World.setBlock(coords.x+1, coords.y-1, coords.z-1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x+1, coords.y, coords.z-1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z-1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x, coords.y-1, coords.z-1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x, coords.y, coords.z-1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z-1, BlockID.dragon_forg_ice_brick, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z-1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x-1, coords.y, coords.z-1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z-1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x-1, coords.y, coords.z, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z, BlockID.dragon_forg_ice_brick, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z+1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x-1, coords.y, coords.z+1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z+1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x, coords.y-1, coords.z+1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x, coords.y, coords.z+1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z+1, BlockID.dragon_forg_ice_brick, 0); 

World.setBlock(coords.x+1, coords.y-1, coords.z+1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x+1, coords.y, coords.z+1, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z+1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x+1, coords.y-1, coords.z, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x+1, coords.y, coords.z, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z, BlockID.dragon_forg_ice_brick, 0); 

World.setBlock(coords.x, coords.y-1, coords.z, BlockID.dragon_forg_ice_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z, BlockID.dragon_forg_ice_brick, 0); 
		}
if(block.id == BlockID.dragon_forg_lightning_core){
		World.setBlock(coords.x+1, coords.y-1, coords.z-1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x+1, coords.y, coords.z-1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z-1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x, coords.y-1, coords.z-1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x, coords.y, coords.z-1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z-1, BlockID.dragon_forg_lightning_brick, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z-1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x-1, coords.y, coords.z-1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z-1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x-1, coords.y, coords.z, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z, BlockID.dragon_forg_lightning_brick, 0); 

World.setBlock(coords.x-1, coords.y-1, coords.z+1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x-1, coords.y, coords.z+1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x-1, coords.y+1, coords.z+1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x, coords.y-1, coords.z+1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x, coords.y, coords.z+1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z+1, BlockID.dragon_forg_lightning_brick, 0); 

World.setBlock(coords.x+1, coords.y-1, coords.z+1, BlockID.dragon_bone_block, 0); 
World.setBlock(coords.x+1, coords.y, coords.z+1, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z+1, BlockID.dragon_bone_block, 0); 

World.setBlock(coords.x+1, coords.y-1, coords.z, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x+1, coords.y, coords.z, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x+1, coords.y+1, coords.z, BlockID.dragon_forg_lightning_brick, 0); 

World.setBlock(coords.x, coords.y-1, coords.z, BlockID.dragon_forg_lightning_brick, 0); 
World.setBlock(coords.x, coords.y+1, coords.z, BlockID.dragon_forg_lightning_brick, 0); 
		}
});

		




// file: Core/Function/Gorgon.js

Callback.addCallback("PlayerAttack", function (player, victim) { 
let item = Player.getCarriedItem(true);
if (item.id == ItemID.cyclops_eye){ 
Entity.addEffect(victim, Native.PotionEffect.weakness, 1000, 180, true, true);
ToolAPI.breakCarriedTool(1);
}
});
	

//gorgon
Callback.addCallback("PlayerAttack", function(player, ent){
  if(Entity.getType(ent) == 12 && Player.getCarriedItem().id == ItemID.gorgon_head){
    const pos = Entity.getPosition(ent);
    World.setBlock(pos.x, pos.y, pos.z, BlockID.pig_stone, 0);
    for(let i = 20; i--;){
      Particles.addParticle(5, pos.x + Math.random() * 0.6 - 0.3, pos.y + Math.random() * 0.6, pos.z + Math.random() * 0.6 - 0.3, Math.random() * 0.02, Math.random() * 0.2, Math.random() * 0.02);
    }
    Entity.remove(ent);
    Game.prevent();
  }
  });
  Callback.addCallback("PlayerAttack", function(player, ent){
if(Entity.getType(ent) == 13 && Player.getCarriedItem().id == ItemID.gorgon_head){
    const pos = Entity.getPosition(ent);
    World.setBlock(pos.x, pos.y, pos.z, BlockID.sheep_stone, 0);
