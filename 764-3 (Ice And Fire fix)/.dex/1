/*function lock*/


Callback.addCallback("ItemUse", function(coords, item, block) {
  if (item.id==ItemID.dread_key && block.id== BlockID.dread_wood_lock) {
 World.destroyBlock(coords.x, coords.y, coords.z, 1)
  World.destroyBlock(coords.x+1, coords.y-1, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x+1, coords.y, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x+1, coords.y+1, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x+1, coords.y+2, coords.z, BlockID.dread_wood, 0);

World.destroyBlock(coords.x-1, coords.y-1, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x-1, coords.y, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x-1, coords.y+1, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x-1, coords.y+2, coords.z, BlockID.dread_wood, 0);

World.destroyBlock(coords.x, coords.y-1, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x, coords.y, coords.z, BlockID.dread_wood, 0);
World.destroyBlock(coords.x, coords.y+1, coords.z, BlockID.dread_wood_lock, 0);
World.destroyBlock(coords.x, coords.y+2, coords.z, BlockID.dread_wood, 0);
  Player.decreaseCarriedItem(1);
}
});









