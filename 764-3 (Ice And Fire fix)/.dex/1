Callback.addCallback("PlayerAttack", function(player, ent){
if(Entity.getType(ent) == 45 && Player.getCarriedItem().id == ItemID.gorgon_head){
    const pos = Entity.getPosition(ent);
    World.setBlock(pos.x, pos.y, pos.z, BlockID.witch_stone, 0);
    for(let i = 20; i--;){
      Particles.addParticle(5, pos.x + Math.random() * 0.6 - 0.3, pos.y + Math.random() * 0.6, pos.z + Math.random() * 0.6 - 0.3, Math.random() * 0.02, Math.random() * 0.2, Math.random() * 0.02);
    }
    Entity.remove(ent);
    Game.prevent();
  }
});

