/*__complete__*/
Callback.addCallback("ServerPlayerTick", function(player, isPlayerDead){
for(let i = 0; i < 36; i++){
let slot = Player.getInventorySlot(i);
    if(slot.id == ItemID.dragon_bone){
    	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "dragon_bone", true);
    };
    if(slot.id == ItemID.scale_red || slot.id == ItemID.scale_gray || slot.id == ItemID.scale_bronze || slot.id == ItemID.scale_green || slot.id == ItemID.scale_blue || slot.id == ItemID.scale_silver || slot.id == ItemID.scale_sapphire || slot.id == ItemID.scale_white || slot.id == ItemID.scale_amythest || slot.id == ItemID.scale_black || slot.id == ItemID.scale_copper || slot.id == ItemID.scale_electric){
    	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "scale", true);
    	};
    if( slot.id == ItemID.fire_dragon_blood || slot.id == ItemID.ice_dragon_blood || slot.id == ItemID.lightning_dragon_blood){
    	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "dragon_blood", true);
    	}
    if( slot.id == ItemID.dragon_steel_fire_ingot || slot.id == ItemID.dragon_steel_ice_ingot || slot.id == ItemID.dragon_steel_lightning_ingot){
    	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "steel_dragon", true);
    	}
   }
});
_recipeComplete_1(ItemID.dragon_sword, "dragon_sword");
_recipeComplete_1(ItemID.dragon_fire_sword, "dragon_sword_blood");
_recipeComplete_1(ItemID.dragon_ice_sword, "dragon_sword_blood");
_recipeComplete_1(ItemID.dragon_lightning_sword, "dragon_sword_blood");
_recipeComplete_1(BlockID.dragon_forg_fire_core_off, "build_forge");
_recipeComplete_1(BlockID.dragon_forg_ice_core_off, "build_forge");
_recipeComplete_1(BlockID.dragon_forg_lightning_core_off, "build_forge");
_recipeComplete_1(BlockID.dragon_forg_fire_brick, "forge_brick");
_recipeComplete_1(BlockID.dragon_forg_ice_brick, "forge_brick");
_recipeComplete_1(BlockID.dragon_forg_lightning_brick, "forge_brick");
_recipeToolsSteel("fire", "ice", "lightning");
}




// file: Core/Gui/Fire Forg.js

var windowForgFire = (function () {
    var width = 1000;
    var height = 1000;
    var size = 45;
    var offset = Math.floor((UI.getScreenHeight() - 440) / 2);
    
    var windowFire = new UI.Window({
        location: { x: 0, y: 0, width: 1000, height: UI.getScreenHeight() },
        drawing: [
            {type: "background", color: Color.argb(90, 0, 0, 0)},
            {type: "bitmap", x: 275, y: offset, bitmap: "inventoryGui", scale: 2.5},
            {type: "bitmap", x: 300, y: offset + 60, bitmap: "forge.background_on", scale: 0.25},
            {type: "bitmap", bitmap: "classic_frame_tab_right", x: 710, y: offset, scale: 2.5}
		
        ],
        elements: (function () {            
            var elements = {
            	progressScale: {type: "scale", x: 306, y: offset + 69, direction: 0, bitmap: "forge.fire_forg_on", scale: 0.25, value: 1},
            	textGui: {type: "text", x: 384, y: offset + 5, width: 120, height: 16, text: "Dragon Forge Fire", font: {color: Color.BLACK, size: 20}},
          
                slotT: {type: "slot", x: 444, y: offset + 90, size: 45, isValid: function(id){ return id == 265}},
		        slotD: {type: "slot", x: 488, y: offset + 90, size: 45, isValid: function(id){ return id == ItemID.fire_dragon_blood}},
		        slot0: {type: "slot", x: 624, y: offset + 90, size: 52, isValid: function(id){ return id == ItemID.dragon_steel_fire_ingot}},
		        closeButton: {type: "closeButton", bitmap: "classic_close_button", bitmap2: "classic_close_button_down", scale: 3, x: 710, y: offset}
		
		
		
               
            };
            for(let i = 0; i < 9; i++){
                 elements["inv" + i] = {
                    type: "invSlot", 
                    x: 293 + 45 * i,
                    y: offset + 386,
                    size: size,
                    index: i
                }
            };
            for(let i = 9; i < 36; i++){
                elements["inv" + i] = {
                    type: "invSlot",
                    x: 293 + 45 * (i % 9),
                    y: offset + 196 + Math.floor(i / 9) * 45,
                    size: size,
                    index: i
                }
            };
            return elements;
        })()
    });
    windowFire.setInventoryNeeded(true);
    windowFire.setBlockingBackground(true);
    
    return windowFire;
})();


TileEntity.registerPrototype(BlockID.dragon_forg_fire_core,{
	
	defaultValues:{
	work:0,
	progress: 0,
	
	},
	
	click: function(id){
if(
    World.getBlockID(this.x,this.y+10,this.z)==BlockID.dragon_forg_fire_core
    
){
return false;
} else{
alert("full");
World.setBlock(this.x+1,this.y,this.z, BlockID.dragon_forg_fire_furnace_fake, 0);
return true;
}
},

	recipe_furnace: function (input1, input2, result, count)
	{
		var ingotSlot = this.container.getSlot("slotT");
		var bloodSlot = this.container.getSlot("slotD");
	
				if(this.data.progress<1000  //1k
				&&ingotSlot.id == input1
				&&bloodSlot.id== input2
				&&bloodSlot.data ==0
				&&ingotSlot.count > 0
				&&bloodSlot.count > 0){          
				this.data.progress++; 
				Particles.addParticle(Native.ParticleType.smoke, this.x, this.y+3, this.z, 0, 0, 0);
				if(this.data.progress >= 1000) {	//1k
				this.container.validateAll();
				this.data.progress = 0;
			
				this.addResult("slot", result, count,0);
				ingotSlot.count --;
				bloodSlot.count--;
                        }
                    }          
				this.container.setScale("progressScale", this.data.progress / 1000);
		},
	
	 tick:function(){
	var ingotSlot = this.container.getSlot("slotT");
		var bloodSlot = this.container.getSlot("slotD");
	
	if(ingotSlot.count<1
		)
	{ ingotSlot.id=0;
	this.data.progress=0
		};
		if(bloodSlot.count<1
		)
	{ bloodSlot.id=0;
	this.data.progress=0
		};
		
		this.recipe_furnace(265,ItemID.fire_dragon_blood,ItemID.dragon_steel_fire_ingot,1);
		},       
         
    addResult: function(area, id, count, data){
        for (var i = 0; i < 2; i++){
            var slot = this.container.getSlot(area + i);
            if (slot.id == id && slot.data == data || slot.id == 0){
                var add = Math.min(64 - slot.count, count);
                slot.count += add;
                slot.id = id;

                slot.data = data;
                count -= add;
                if (count == 0){
                    break;
                }
            }
        }
        if (count > 0){
            World.drop(this.x + .5, this.y + 1, this.z + .5, id, count, data);
        }
    }, 
    getGuiScreen: function () {
        return windowForgFire;
    }
});




// file: Core/Gui/Ice Forg.js



var windowForgIce = (function () {
    var width = 1000;
    var height = 1000;
    var size = 45;
    var offset = Math.floor((UI.getScreenHeight() - 440) / 2);
    
    var windowIce = new UI.Window({
        location: { x: 0, y: 0, width: 1000, height: UI.getScreenHeight() },
        drawing: [
            {type: "background", color: Color.argb(90, 0, 0, 0)},
            {type: "bitmap", x: 275, y: offset, bitmap: "inventoryGui", scale: 2.5},
            {type: "bitmap", x: 300, y: offset + 60, bitmap: "forge.background_on", scale: 0.25},
            {type: "bitmap", bitmap: "classic_frame_tab_right", x: 710, y: offset, scale: 2.5}
        ],
        elements: (function () {            
            var elements = {
            	progressScale: {type: "scale", x: 306, y: offset + 69, direction: 0, bitmap: "forge.ice_forg_on", scale: 0.25, value: 1},
            	textGui: {type: "text", x: 384, y: offset + 5, width: 120, height: 16, text: "Dragon Forge Ice", font: {color: Color.BLACK, size: 20}},
                slotT: {type: "slot", x: 444, y: offset + 90, size: 45, isValid: function(id){ return id == 265}},
		        slotD: {type: "slot", x: 488, y: offset + 90, size: 45, isValid: function(id){ return id == ItemID.ice_dragon_blood}},
		        slot0: {type: "slot", x: 624, y: offset + 90, size: 52, isValid: function(id){ return id == ItemID.dragon_steel_ice_ingot}},
		        closeButton: {type: "closeButton", bitmap: "classic_close_button", bitmap2: "classic_close_button_down", scale: 3, x: 710, y: offset}
		        
            };
            for(let i = 0; i < 9; i++){
                 elements["inv" + i] = {
                    type: "invSlot", 
                    x: 293 + 45 * i,
                    y: offset + 386,
                    size: size,
                    index: i
                }
            };
            for(let i = 9; i < 36; i++){
                elements["inv" + i] = {
                    type: "invSlot",
                    x: 293 + 45 * (i % 9),
                    y: offset + 196 + Math.floor(i / 9) * 45,
                    size: size,
                    index: i
                }
            };
            return elements;
        })()
    });
    windowIce.setInventoryNeeded(true);
    windowIce.setBlockingBackground(true);
    
    return windowIce;
})();



TileEntity.registerPrototype(BlockID.dragon_forg_ice_core,{
	
	defaultValues:{
	work:0,
	progress: 0,
	
	},
	click: function(id){
if(
    World.getBlockID(this.x,this.y+10,this.z)==BlockID.dragon_forg_fire_core
    
){
return false;
} else{
alert("full");
World.setBlock(this.x+1,this.y,this.z, BlockID.dragon_forg_ice_furnace_fake, 0);

return true;
}
},
	recipe_furnace: function (input1, input2, result, count)
	{
		var ingotSlot = this.container.getSlot("slotT");
		var bloodSlot = this.container.getSlot("slotD");
	
				if(this.data.progress<1000
				&&ingotSlot.id == input1
				&&bloodSlot.id== input2
				&&bloodSlot.data ==0
				&&ingotSlot.count > 0
				&&bloodSlot.count > 0){          
				this.data.progress++; 
				if(this.data.progress >= 1000){	
				this.container.validateAll();
				this.data.progress = 0;
			
				this.addResult("slot", result, count,0);
				ingotSlot.count --;
				bloodSlot.count--;
                        }
                    }          
				this.container.setScale("progressScale", this.data.progress / 1000);
		},
	
	 tick:function(){
	var ingotSlot = this.container.getSlot("slotT");
		var bloodSlot = this.container.getSlot("slotD");
	
	if(ingotSlot.count<1
		)
	{ ingotSlot.id=0;
	this.data.progress=0
		};
		if(bloodSlot.count<1
		)
	{ bloodSlot.id=0;
	this.data.progress=0
		};
		
		this.recipe_furnace(265,ItemID.ice_dragon_blood,ItemID.dragon_steel_ice_ingot,1);
		
		},       
         
    addResult: function(area, id, count, data){
        for (var i = 0; i < 2; i++){
            var slot = this.container.getSlot(area + i);
            if (slot.id == id && slot.data == data || slot.id == 0){
                var add = Math.min(64 - slot.count, count);
                slot.count += add;
                slot.id = id;

                slot.data = data;
                count -= add;
                if (count == 0){
                    break;
                }
            }
        }
        if (count > 0){
            World.drop(this.x + .5, this.y + 1, this.z + .5, id, count, data);
        }
    },  
    getGuiScreen: function () {
        return windowForgIce;
    }
});








// file: Core/Gui/Lightning Forg.js

var windowForgLightning = (function () {
    var width = 1000;
    var height = 1000;
    var size = 45;
    var offset = Math.floor((UI.getScreenHeight() - 440) / 2);
    
    var windowLightning  = new UI.Window({
        location: { x: 0, y: 0, width: 1000, height: UI.getScreenHeight() },
        drawing: [
            {type: "background", color: Color.argb(90, 0, 0, 0)},
            {type: "bitmap", x: 275, y: offset, bitmap: "inventoryGui", scale: 2.5},
            {type: "bitmap", x: 300, y: offset + 60, bitmap: "forge.background_on", scale: 0.25},
            {type: "bitmap", bitmap: "classic_frame_tab_right", x: 710, y: offset, scale: 2.5}
        ],
        elements: (function () {            
            var elements = {
            	progressScale: {type: "scale", x: 306, y: offset + 69, direction: 0, bitmap: "forge.lightning_forg_on", scale: 0.25, value: 1},
            	textGui: {type: "text", x: 384, y: offset + 5, width: 120, height: 16, text: "Dragon Forge Lightning", font: {color: Color.BLACK, size: 20}},
                slotT: {type: "slot", x: 444, y: offset + 90, size: 45, isValid: function(id){ return id == 265}},
		        slotD: {type: "slot", x: 488, y: offset + 90, size: 45, isValid: function(id){ return id == ItemID.lightning_dragon_blood}},
		        slot0: {type: "slot", x: 624, y: offset + 90, size: 52, isValid: function(id){ return id == ItemID.dragon_steel_lightning_ingot}},
		        closeButton: {type: "closeButton", bitmap: "classic_close_button", bitmap2: "classic_close_button_down", scale: 3, x: 710, y: offset}
		
		
		
            	
            };
            for(let i = 0; i < 9; i++){
                 elements["inv" + i] = {
                    type: "invSlot", 
                    x: 293 + 45 * i,
                    y: offset + 386,
                    size: size,
                    index: i
                }
            };
            for(let i = 9; i < 36; i++){
