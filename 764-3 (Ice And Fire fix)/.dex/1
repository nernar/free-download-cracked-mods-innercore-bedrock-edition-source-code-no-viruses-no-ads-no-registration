/*__OreFunction__*/
Block.registerDropFunction("silver_ore", function(coords, blockID, blockData, level, enchant){
	if(level > 2){
		if(enchant.silk){
			return [[blockID, 1, 0]];
		}
		return [[BlockID.silver_ore, 1, 0]]
	}
	return [];
}, 3);

Block.registerDropFunction("copper_ore", function(coords, blockID, blockData, level, enchant){
	if(level > 2){
		if(enchant.silk){
			return [[blockID, 1, 0]];
		}
		return [[BlockID.copper_ore, 1, 0]]
	}
	return [];
}, 3);


Block.registerDropFunction("amythest_ore", function(coords, blockID, blockData, level, enchant){
	if(level > 2){
		if(enchant.silk){
			return [[blockID, 1, 0]];
		}
		ToolAPI.dropOreExp(coords, 3, 7, enchant.experience);
		return [[ItemID.amythest_gem, 1, 0]]
	}
	return [];
}, 3);

Block.registerDropFunction("sapphire_ore", function(coords, blockID, blockData, level, enchant){
	if(level > 2){
		if(enchant.silk){
			return [[blockID, 1, 0]];
		}
		ToolAPI.dropOreExp(coords, 3, 7, enchant.experience);
		return [[ItemID.sapphire, 1, 0]]
	}
	return [];
}, 3);
/*__Spawn__*/





Callback.addCallback("GenerateChunkUnderground", function (chunkX, chunkZ, random) {
if(__config__.get("ore.copper")){
        for (var i = 7; i < __config__.get("ore.height_copper_ore"); i++) {
            var coords = GenerationUtils.randomCoords(chunkX, chunkZ, 1, i);
            GenerationUtils.generateOre(coords.x, coords.y, coords.z, BlockID.copper_ore, 0, __config__.get("ore.chance_copper_ore"), false, random.nextInt());
        }
    }
    if (__config__.get("ore.silver")) {
        for (var i = 7; i < __config__.get("ore.height_silver_ore"); i++) {
            var coords = GenerationUtils.randomCoords( chunkX, chunkZ, 1, i);
            GenerationUtils.generateOre(coords.x, coords.y, coords.z, BlockID.silver_ore, 0, __config__.get("ore.chance_silver_ore"), false, random.nextInt());
        }
    }
    if (__config__.get("ore.amythest")) {
        for (var i = 7; i < __config__.get("ore.height_amythest_ore"); i++) {
            var coords = GenerationUtils.randomCoords( chunkX, chunkZ, 1, i);
            GenerationUtils.generateOre(coords.x, coords.y, coords.z, BlockID.amythest_ore, 0, __config__.get("ore.chance_amythest_ore"), false, random.nextInt());
        }
    }
    if (__config__.get("ore.sapphire")) {
        for (var i = 7; i < __config__.get("ore.height_sapphire_ore"); i++) {
            var coords = GenerationUtils.randomCoords( chunkX, chunkZ, 1, i);
            GenerationUtils.generateOre(coords.x, coords.y, coords.z, BlockID.sapphire_ore, __config__.get("ore.chance_sapphire_ore"), 9, false, random.nextInt());
        }
    }
});




