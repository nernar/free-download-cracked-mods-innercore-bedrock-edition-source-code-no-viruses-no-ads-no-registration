var ForgeApi = {
	
	ForgeFire: function (coords, player) {
		var fire_forge = World.getBlock;
		
        var fire_forge1 = fire_forge(coords.x+1, coords.y-1, coords.z-1);
		var fire_forge2 = fire_forge(coords.x+1, coords.y, coords.z-1);
        var fire_forge3 = fire_forge(coords.x+1, coords.y+1, coords.z-1);
        
        var fire_forge4 = fire_forge(coords.x, coords.y-1, coords.z-1);
