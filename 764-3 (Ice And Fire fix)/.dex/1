let RecipesScrutiny = {
	all: {},
	crafts: [],
	isCrafts(player, items){
		if(!this.all[player])
			return false;
		let recipe = this.all[player].slice(0);
		for(let i in items){
			if(recipe.indexOf(items[i]) == -1)
				return false 
			else
					recipe[recipe.indexOf(items[i])] = -1;
		}
		return true;
	},
	addCrafts(items){
		this.crafts.push(items);
	}
};
const _recipeComplete_1 = function(id, nameAch){
	Callback.addCallback("VanillaWorkbenchCraft", function(result, container, player){
	RecipesScrutiny.all[player] = RecipesScrutiny.all[player] || [];
	RecipesScrutiny.all[player].push(result.id);
	if(RecipesScrutiny.isCrafts(player, [id]))
	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", nameAch, true);
   });
};
const _recipeToolsSteel = function(id1, id2, id3){
	Callback.addCallback("VanillaWorkbenchCraft", function(result, container, player){
	RecipesScrutiny.all[player] = RecipesScrutiny.all[player] || [];
	RecipesScrutiny.all[player].push(result.id);
	if(RecipesScrutiny.isCrafts(player, [ItemID["dragon_sword_steel_" + id1], ItemID["dragon_sword_steel_" + id2], ItemID["dragon_sword_steel_" + id3]]))
	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "steel_sword", true);
	if(RecipesScrutiny.isCrafts(player, [ItemID["dragon_shovel_steel_" + id1], ItemID["dragon_shovel_steel_" + id2], ItemID["dragon_shovel_steel_" + id3]]))
	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "steel_shovel", true);
	if(RecipesScrutiny.isCrafts(player, [ItemID["dragon_pickaxe_steel_" + id1], ItemID["dragon_pickaxe_steel_" + id2], ItemID["dragon_pickaxe_steel_" + id3]]))
	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "steel_pickaxe", true);
	if(RecipesScrutiny.isCrafts(player, [ItemID["dragon_axe_steel_" + id1], ItemID["dragon_axe_steel_" + id2], ItemID["dragon_axe_steel_" + id3]]))
	ScrutinyAPI_V1.giveScrutiny(player, "ice_and_fire", "i_a_f", "steel_axe", true);
   });
};







