// file: Core/Block/Myrmex Block.js

var BLOCK_TYPE_LEAVES = Block.createSpecialType({
    base: 18,
    destroytime: 0.1,
    explosionres: 1,
    renderallfaces: true,
    renderlayer: 1,
    lightopacity: 1,
    translucency: 0.5,
    sound: "grass"
});
const render_model = new ICRender.CollisionShape();
render_model.addEntry().addBox(1, 1, 1, 0, 0, 0);
/*__desert__*/
