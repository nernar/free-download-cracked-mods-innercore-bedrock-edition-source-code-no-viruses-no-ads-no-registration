// file: Core/Function/Food.js

Callback.addCallback("FoodEaten", function(heal, satRatio, player, coords, itemFood) {
	var coords = Entity.getPosition(player);
	var player = Player.get();
	var itemFood = Player.getCarriedItem();
	if (itemFood.id == ItemID.fire_dragon_meat) {
	Entity.setFire(player, 300, true);
	}
    if (itemFood.id == ItemID.ice_dragon_meat) {
    	Entity.addEffect(player, Native.PotionEffect.movementSlowdown, 9, 200, false, false);
    
    }
    if (itemFood.id == ItemID.lightning_dragon_meat){
    	Entity.spawn(coords.x, coords.y-1, coords.z, 93);
        Entity.setFire(player, 100, true);
        World.playSoundAtEntity(player, "ambient.weather.thunder", 20);
    }
});















